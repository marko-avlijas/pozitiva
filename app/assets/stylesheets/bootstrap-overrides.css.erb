/*-------- NAVBAR --------- */

.navbar{
	background-color: #82a536;
	border-bottom: 5px solid #68842b;
	border-radius: 0;
}
.navbar-default .navbar-nav > li > a{
	color: #fff;
  font-size: 16px;
}
.navbar-default .navbar-nav > li > a:hover {
	color: #333;
}
.navbar-default .navbar-brand {
  color: #fff;
  font-size: 18px;
  font-weight: 600;
}
.navbar-default .navbar-nav > .active > a, .navbar-default .navbar-nav > .active > a:hover, .navbar-default .navbar-nav > .active > a:hover {
  color: #fff;
  font-weight: bold;
}
.navbar-default .navbar-brand:hover {
  color: #333;
}
.navbar-default .navbar-nav > .active > a, .navbar-default .navbar-nav > .active > a:hover, .navbar-default .navbar-nav > .active > a:focus{
  background-color: #718F2F;
  border-right: 1px solid #5B791B;
  border-left: 1px solid #5B791B;
  border-bottom: 1px solid #8EB145;
}
.navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus{
  background-color: #78A022;
}
.navbar-default .navbar-nav > .dropdown > a .caret {
  border-top-color: #fff;
  border-bottom-color: #fff;
}
.navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus {
  background-color: transparent;
  color: #333;
}
.navbar-default .navbar-toggle:hover, .navbar-default .navbar-toggle:focus {
  background-color: #979B91;
}
.navbar-nav.navbar-right:last-child {
  margin-right: 0;
}
.navbar-default .navbar-toggle .icon-bar {
  background-color: #FFF;
}
.navbar-default .navbar-toggle:hover, .navbar-default .navbar-toggle:focus {
  background-color: #678A1D;
}
.navbar-default .navbar-nav > .open > a:focus, .navbar-default .navbar-nav > .open > a:visited {
color: #F6F8EE;
text-shadow: none;
}
.navbar-default .navbar-collapse, .navbar-default .navbar-form {
border-color: #fff;
}
.dropdown-menu .divider {
background-color: #979B91;
}
.navbar-collapse {
max-height: 500px;
}

.dropdown-menu > li > a{
  color: #333;
}
.dropdown-menu > li > a:hover{
  background-color: #718F2F;
  color: #fff;
}
.dropdown-menu .divider{
	background-color: #d4d6cd;
}
.navbar-brand{
  font-family: 'Viga', sans-serif;
  font-size: 22px;
}


/*--------------------------- TABS ------------------------------*/

.nav-tabs {
border-bottom: 1px solid #E2DECE;
}
.nav-tabs > li > a {
  background-color: #F6F5F1;
  border: 1px solid #E2DECE;

}
.nav-tabs > li > a:hover {
  background-color: #EBE7DC;
  border: 1px solid #D3CFBD;
}
.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus {
color: #525252;
cursor: default;
background-color: #FFF;
border: 1px solid #E2DECE;
border-bottom-color: rgba(0, 0, 0, 0);
}

/*-------------------------- BUTTONS ----------------------------*/

.btn {
display: inline-block;
text-align: center;
vertical-align: middle;
cursor: pointer;
color: #59603E;
font-size: 16px;
text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
background-color: #E2E5D7;
background-image: -moz-linear-gradient(top, #ebede3, #d5d9c4);
background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#EBEDE3), to(#D5D9C4));
background-image: -webkit-linear-gradient(top, #EBEDE3, #D5D9C4);
background-image: -o-linear-gradient(top, #ebede3, #d5d9c4);
background-image: linear-gradient(to bottom, #EBEDE3,#D5D9C4);
background-repeat: repeat-x;
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffebede3', endColorstr='#ffd5d9c4', GradientType=0);
border-color: #D5D9C4 #D5D9C4 #B4BB95;
border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
filter: progid:DXImageTransform.Microsoft.gradient(enabled = false);
border: 1px solid #BBB;
border-bottom-color: #A2A2A2;
-webkit-border-radius: 4px;
-moz-border-radius: 4px;
border-radius: 4px;
-webkit-border-radius: 4px;
-moz-border-radius: 4px;
border-radius: 4px;
-webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2),0 1px 2px rgba(0, 0, 0, 0.05);
-moz-box-shadow: inset 0 1px 0 rgba(255,255,255,0.2),0 1px 2px rgba(0,0,0,0.05);
box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2),0 1px 2px rgba(0, 0, 0, 0.05);
}

.btn-primary{
  color:#ffffff;
  text-shadow:0 -1px 0 rgba(0,0,0,0.25);
  background-color:#6e8d2d;
  background-image:-moz-linear-gradient(top, #6e8d2d, #617c28);
  background-image:-webkit-gradient(linear, 0 0, 0 100%, from(#6e8d2d), to(#617c28));
  background-image:-webkit-linear-gradient(top, #6e8d2d, #617c28);
  background-image:-o-linear-gradient(top, #6e8d2d #617c28);
  background-image:linear-gradient(to bottom, #6e8d2d,#617c28);
  background-repeat:repeat-x;
  filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff7d8176', endColorstr='#ff70746a', GradientType=0);
  border-color:#70746a #70746a #494c45;
  border-color:rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);*
  background-color:#70746a;
  filter:progid:DXImageTransform.Microsoft.gradient(enabled = false);
  font-size: 16px;
  font-weight: 600;
}
.btn-primary:hover, .btn-primary:focus, .btn-primary:active{
  color:#ffffff;
  background-color:#617c28;
  text-decoration: none;
  background-position: 0 -15px;
  -webkit-transition: background-position 0.1s linear;
  -moz-transition: background-position 0.1s linear;
  -ms-transition: background-position 0.1s linear;
  -o-transition: background-position 0.1s linear;
  transition: background-position 0.1s linear;
  border-color:transparent;
}
.btn-primary:active, .btn-primary.active{
  background-color:#617c28;
}
.btn-xs{
	padding: 3px 6px;
	font-size: 13px;
	line-height: 20px;
}

/*------------------------------- PANEL ---------------------------*/

.white-card {
padding: 10px;
border: 1px solid #D3CCC4;
border-radius: 4px;
-webkit-box-shadow: 0px 1px 4px 1px #DBD9D6, 2px 2px 1px 0px #FFF inset;
-moz-box-shadow: 0px 1px 4px 1px #DBD9D6, 2px 2px 1px 0px #fff inset;
box-shadow: 0px 1px 4px 1px #DBD9D6, 2px 2px 1px 0px #FFF inset;
margin-bottom: 20px;
border-radius: 4px;
background-color: #FFF;
}
.white-card.white-card-large{
	padding: 20px 40px;
}
.white-card .panel-heading{
	background-color: transparent;
	border-color: transparent;
}
.white-card .panel-heading h3{
	font-weight: 400;
	color: #31302E;
	font-size: 18px;
	margin-top: 0px;
	margin-bottom: 10px;
	padding: 20px 0;
	border-bottom: 1px solid #CFD1C6;
	-webkit-box-shadow: 0px 1px 0px #FFF;
	-moz-box-shadow: 0px 1px 0px #ffffff;
	box-shadow: 0px 1px 0px #FFF;
	text-shadow: -1px -1px #FFF;
}
.white-card .panel-footer{
	border-top: 1px solid #DFE1D6;
	background-color: transparent;
	-webkit-box-shadow: 0px 1px 0px 0px #FFF;
	-moz-box-shadow: 0px 1px 0px 0px #ffffff;
	box-shadow: 0px 1px 0px 0px #FFF;
	padding-bottom: 15px;
}
.panel{
	border: 1px solid #D3CCC4;
	border-radius: 4px;
	-webkit-box-shadow: 0px 1px 4px 1px #DBD9D6, 2px 2px 1px 0px #FFF inset;
	-moz-box-shadow: 0px 1px 4px 1px #DBD9D6, 2px 2px 1px 0px #fff inset;
	box-shadow: 0px 1px 4px 1px #DBD9D6, 2px 2px 1px 0px #FFF inset;
}
.panel .panel-heading, .panel .panel-footer {
  position: relative;
}
.panel .panel-heading h5 a:hover, .panel .panel-heading h4 a:hover{
	text-decoration: underline;
}
.panel .panel-heading span.author{
	line-height: 50px;
  font-size: 16px;
}
.panel .offer-title{
  background-color: #f6f5f1;
  border-radius: 0;
  border-bottom: 1px solid #E0DED5;
}
.panel-heading h3{
	margin: 0;
}
.panel-heading.offer-title h3{
	line-height: 50px;;
}
.panel-offer .panel-heading, .panel-offer .panel-body, .panel-offer .panel-footer{
  padding: 20px 30px;
}

.panel-heading h3 img, .panel-heading h5 img{
	width: 50px;
	height: 50px;
	margin-right: 10px;
	float: left;
}
.panel .panel-body{
	position: relative;
}
.panel .panel-body ul{
  padding-top: 0;
  padding-bottom: 0;
}
.panel .panel-body .focus {
border: 1px solid #E0DED5;
background-color: #FCFCFC;
border-radius: 4px;
position: relative;
}
.focus-header, .focus-body, .focus-footer{
  padding: 10px 20px;
}
.focus-header, .focus-footer{
  background-color: #F6F5F1;
}
.focus-header img{
  width: 50px;
}
.focus-header h4{
  margin: 0;
}

.panel.panel-offer .panel-footer{
  position: relative;
  background-color: #f6f5f1;
  border-top: 1px solid #E0DED5;
}
.footer-links{
	padding: 10px 0;
}
.footer-links a, .footer-links button{
  margin-bottom: 10px;
  display: inline-block;
}
.panel.panel-offer .focus{
  margin: 10px 30px 0 0;
}

/*---------------------------------- ERRORS AND ALLERTS -------------------------*/

.alert-danger {
	color: #A73333;
	background-color: #F8E6DD;
	border-color: #CF8F85;
	opacity: .8;
	  filter: alpha(opacity=80);
-webkit-box-shadow: 0px 1px 4px 1px #B4B6A9;
-moz-box-shadow: 0px 1px 4px 1px #b4b6a9;
box-shadow: 0px 1px 4px 1px #B4B6A9;
}
.alert-success {
	color: #344B11;
	background-color: #CFE2A3;
	border-color: #95AF81;
	opacity: .8;
	  filter: alpha(opacity=80);
-webkit-box-shadow: 0px 1px 4px 1px #B4B6A9;
-moz-box-shadow: 0px 1px 4px 1px #b4b6a9;
box-shadow: 0px 1px 4px 1px #B4B6A9;
}
.form-control:focus {
