.offer .glyphicon.glyphicon-ban-circle:hover {
  color: #cc2200;
}

.help-block .glyphicon.glyphicon-ban-circle:hover {
  color: #ccc;
}
.disabled .glyphicon.glyphicon-ban-circle {
  color: #cc2200;
  position: relative;
  top: 2px;
}
.disabled .glyphicon.glyphicon-ban-circle:hover {
  color: #777;
}
.disabled td, .disabled .row > div:not(.qty), .disabled .input-group {
  opacity: .4;
}
.disabled td:first-child{
  opacity: 1;
}
.disabled .fa {
  color: #777 !important;
}
.disabled .glyphicon-ban-circle.disabled-order {
  color: #777
}



