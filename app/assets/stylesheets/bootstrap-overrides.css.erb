body{
  background-color: #f4f3ef;
  color: #525252;
  padding-top: 80px;
  font-family: 'Source Sans Pro', sans-serif;
  font-size: 16px;
}
a{
	color: #718F2F;
  font-weight: 600;
}
a:hover{
	color: #5B791B;
  text-decoration: none;
}
a:active{
	color: #5B791B;
}
ul{
	list-style-type: none;
	padding-left: 20px;
}

h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6 {
font-weight: 500;
}


#wrap {
  margin: 0 auto;
  padding: 0 0 20px;
}

#footer {
  height: 70px;
  color: #CAB5B1;
  background-color: #82A536;
  border-top: 5px solid #68842B;
}

#footer a{
  color: #EBEDE3;
}
#footer a:hover {
